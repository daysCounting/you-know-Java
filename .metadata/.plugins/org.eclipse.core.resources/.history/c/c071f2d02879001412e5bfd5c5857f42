package FINAL;

import java.util.Date;
import java.util.Properties;

interface testMemory{
	
	Properties p  = System.getProperties();
	p.list(System.out);
	System.out.println(" - - - Memory Usage:");
    Runtime rt  = Runtime.getRuntime();
    System.out.println("Total Memory = "
    									+ rt.totalMemory()
    									+ "  Free Memory  = "
    									+ rt.freeMemory());
}
